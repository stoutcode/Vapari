<?xml version="1.0" encoding="UTF-8"?>

<?import fi.jyu.mit.fxgui.ComboBoxChooser?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxVapari.VapariToimintoController">
   <bottom>
      <HBox alignment="CENTER" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="toimintoTallenna" mnemonicParsing="false" onAction="#handleToimintoTallenna" prefWidth="75.0" text="Tallenna" />
            <Button fx:id="toimintoPeruuta" mnemonicParsing="false" onAction="#handleToimintoPeruuta" prefWidth="75.0" text="Peruuta" />
         </children>
         <BorderPane.margin>
            <Insets top="15.0" />
         </BorderPane.margin></HBox>
   </bottom>
   <top>
      <Label text="Toiminto" BorderPane.alignment="CENTER_LEFT">
         <BorderPane.margin>
            <Insets bottom="14.0" />
         </BorderPane.margin></Label>
   </top>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <center>
      <ComboBoxChooser fx:id="toimintoToiminto" prefWidth="150.0" rivit="" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
